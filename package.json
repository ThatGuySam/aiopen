{
  "name": "aiopen",
  "type": "module",
  "version": "0.0.1-beta.2",
  "packageManager": "pnpm@10.2.1",
  "description": "Portable Mock Server for OpenAI's API",
  "author": "Sam Carlton <@ThatGuySam>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/ThatGuySam",
  "homepage": "https://github.com/ThatGuySam/aiopen#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThatGuySam/aiopen.git"
  },
  "bugs": "https://github.com/ThatGuySam/aiopen/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "aiopen": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "node dist/index.mjs",
    "mock-bin": "nr build && cross-env NODE_ENV=development node dist/index.mjs",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "generate-orval": "pnpm tsx ./scripts/timed-orval.ts",
    "postgenerate-orval": "pnpm tsx ./scripts/orval-cleanup.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.0",
    "axios": "^1.7.9",
    "msw": "^2.7.0",
    "pathe": "^2.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^8.1.0",
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.0",
    "lint-staged": "^15.4.3",
    "orval": "^7.5.0",
    "pnpm": "^10.2.1",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
